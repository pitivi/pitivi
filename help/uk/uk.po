msgid "@@image: 'figures/fadestep1.png'; md5=4bdb320d2ef57e3d09c67fcdba8ab103"
msgstr "@@image: 'figures/fadestep1.png'; md5=4bdb320d2ef57e3d09c67fcdba8ab103"

